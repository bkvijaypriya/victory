<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
    xsi:schemaLocation = "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0 ">
    
    
   <!-- Auto Wiring by Name -->
   <bean name ="student" class = "Student" autoWire = "byName">
       <Property name="studentName" value ="VP" />
   </bean>
   <bean name="javaBook" class="Book">
       <Property name= "title" value="Java book" />
   </bean>
   
   <!-- Auto Wiring by Type -->
   <bean name="car" class="Car" autowrire="byType">
       <bean name="alloyWheel" calss="Wheel" />
       <property name="name" value ="Alloy WheelVP" />
   </bean>
   
   <!-- Auto Wiring by Constructor -->
   <!--  bean name="collage" class = "Collage" autowire="constructor" /-->
</beans>
   
   